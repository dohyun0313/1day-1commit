def solution(num_list):
    total1 = 0
    total2 = 0
    count = 0
    for char in num_list : 
        if count == 0 :
            total1 += char
            count = 1
        else : 
            total2 += char
            count = 0
    if total1 > total2 : return total1
    else : return total2
    
