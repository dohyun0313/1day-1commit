def solution(binomial):
    binomial = binomial.split()
    answer = 0
    if binomial[1] == "+" :
        answer = int(binomial[0])+int(binomial[2])
    elif binomial[1] == "-" :
        answer = int(binomial[0])-int(binomial[2])
    else : 
        answer = int(binomial[0])*int(binomial[2])
    return answer
